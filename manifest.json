{
    "manifest_version": 3,
    "name": "Chrome Bookmarks",
    "version": "1.0",
    "description": "List all bookmarks on new tab.",
    "permissions": ["bookmarks"],
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  